// manifest.json

{
  "manifest_version": 2,
  "name": "BTS Cursor Extension",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["popupv4.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "bts.png"
  }
}
